        "# 惑星のリスト\n",
        "planets = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# for文を使った繰り返し\n",
        "print(\"【for文】\")\n",
        "for planet in planets:\n",
        "    print(planet)\n",
        "\n",
        "for _ in range(2):\n",
        "    for planet in planets:\n",
        "       print(planet)\n"
