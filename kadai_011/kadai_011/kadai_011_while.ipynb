        "# 惑星のリスト\n",
        "planets = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# while文を使った繰り返し\n",
        "print(\"【while文】\")\n",
        "\n",
        "count = 0\n",
        "while count < 2:\n",
        "  for planet in  planets:\n",
        "      print(planet)\n",
        "  count += 1\n"
      ],
